
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Automation Practice Test Suite" parallel="methods"
	thread-count="2" verbose="1">

	<!-- âœ… Global TestNG Listener -->
	<listeners>
		<listener class-name="com.ui.listener.TestListener" />
	</listeners>

	<test name="UI Automation Test">
		<parameter name="browser" value="${browser:chrome}" />
		<parameter name="isLambdaTest" value="${isLambdaTest:false}" />
		<parameter name="isHeadless" value="${isHeadless:false}" />

		<classes>
			<class name="com.ui.tests.InvalidCredLoginTest" />
			<class name="com.ui.tests.LoginTest" />
			<class name="com.ui.tests.AddNewFirstAddressTest" />
			<class name="com.ui.tests.SearchProductTest" />
			

		</classes>
	</test>

</suite>